/* Corpo da página */
body {
  background-color: #1a1a1a;
  color: #f0f0f0;
  font-family: 'Segoe UI', sans-serif;
  text-align: center;
  margin: 0;
  padding: 20px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  min-height: 100vh;
  overflow: hidden;
}

html {
  overflow-y: hidden;
  overflow-x: hidden;
}

/* Título */
h1 {
  margin-bottom: 10px;
  color: #ffcc00;
  text-shadow: 1px 1px 2px #000;
}

/* Game title centered on screen */
#game-title {
  position: absolute; /* place relative to viewport, under the scoreboard */
  top: 85px; /* sits below the scoreboard */
  left: 50%;
  transform: translateX(-50%);
  z-index: 9; /* just under the scoreboard (which uses z-index:10) */
  color: #ffdd33;
  /* increased font size for desktop */
  font-size: 22px;
  font-weight: 600;
  font-family: inherit;
  letter-spacing: 2.5px;
  text-transform: uppercase;
  text-shadow: 0 4px 20px rgba(0,0,0,0.85), 0 1px 0 rgba(255,255,255,0.05);
  pointer-events: none;
  mix-blend-mode: normal;
  padding: 4px 10px;
}

/* Sobrescrever para desktop - garantir que fique próximo ao placar */
@media (min-width: 901px) {
  #game-title {
    top: 85px !important;
  }
}

/* Responsividade do título para tablets e celulares */
@media (max-width: 900px) {
  #game-title {
    font-size: 14px !important;
    letter-spacing: 1.5px !important;
    top: auto !important;
    position: static !important;
    transform: none !important;
    margin: -3px auto 0 auto !important; /* Sobrepondo - margem negativa */
    display: block !important;
    padding: 3px 8px !important;
  }
}

/* Título ainda menor para celulares muito pequenos */
@media (max-width: 480px) {
  #game-title {
    font-size: 11px !important;
    letter-spacing: 1px !important;
    padding: 2px 6px !important;
    margin: -2px auto 0 auto !important; /* Sobrepondo - margem negativa */
  }
}

/* Título para telas 800x600 */
@media (min-width: 800px) and (max-width: 800px) and (min-height: 600px) and (max-height: 600px) {
  #game-title {
    font-size: 16px !important;
    letter-spacing: 1.8px !important;
    top: auto !important;
    position: static !important;
    transform: none !important;
    margin: -3px auto 0 auto !important; /* Sobrepondo - margem negativa */
    display: block !important;
  }
}

/* Title uses fixed 14px across all viewports; no responsive overrides */

/* Canvas do jogo */
canvas {
  border: 5px solid #fff;
  margin-bottom: 25px;
  margin-top: -60px;
  box-shadow: 0 0 25px rgba(0,0,0,0.7);
  border-radius: 3px;
  max-width: 95%;
  height: auto;
}

/* Placar com efeito */
#placar {
  font-size: 20px;
  font-weight: bold;
  color: #fff;
  background: linear-gradient(90deg, #444, #222);
  border: 2px solid #ffcc00;
  border-radius: 10px;
  padding: 10px 20px;
  margin: 10px auto;
  width: fit-content;
  box-shadow: 0 0 15px #ffcc00;
  text-shadow: 1px 1px 2px #000;
  transition: all 0.3s ease;
  animation: brilho 2s infinite alternate;
}

/* Animação de brilho */
@keyframes brilho {
  0% {
    box-shadow: 0 0 10px #ffcc00;
  }
  100% {
    box-shadow: 0 0 20px #ffcc00;
  }
}

/* Media query específica para celulares em modo paisagem */
@media (max-width: 900px) and (orientation: landscape) and (max-height: 500px) {
  body {
    padding-bottom: 18vh;
  }

  canvas {
    max-width: 65vw;
    max-height: 45vh;
    margin-top: 1vh;
  }

  .placar-futebol {
    width: 85vw;
