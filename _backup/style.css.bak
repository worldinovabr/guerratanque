/* Menu de seleção de aviões */
.plane-menu {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: linear-gradient(135deg, #0a0a0a 0%, #1a1a2e 50%, #0a0a0a 100%);
  display: grid;
  grid-template-rows: auto 1fr auto;
  justify-content: center;
  align-items: center;
  z-index: 9999;
  padding: 20px;
  gap: 20px;
  animation: fadeIn 0.5s ease;
  box-sizing: border-box;
}

/* Theme variables: glass style */
:root {
  --glass-bg-start: rgba(15, 18, 22, 0.65);
  --glass-bg-end: rgba(10, 12, 14, 0.36);
  --glass-border: rgba(255, 255, 255, 0.06);
  --glass-shadow: 0 8px 30px rgba(0,0,0,0.55);
  --glass-blur: 10px;
}

@keyframes fadeIn {
  from { opacity: 0; }
  to { opacity: 1; }
}

.plane-menu h2 {
  color: #fff;
  font-size: 3rem;
  margin: 0;
  text-shadow: 0 0 20px rgba(76, 175, 80, 0.5), 2px 2px 4px rgba(0, 0, 0, 0.8);
  font-weight: 900;
  letter-spacing: 2px;
  text-transform: uppercase;
  background: linear-gradient(45deg, #4CAF50, #8BC34A);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  background-clip: text;
}

.plane-options {
  display: grid;
  grid-template-columns: repeat(6, 1fr);
  gap: 20px;
  max-width: 1400px;
  padding: 20px;
}

.plane-option {
  cursor: pointer;
  padding: 25px 20px;
  background: linear-gradient(145deg, rgba(30, 30, 40, 0.9), rgba(20, 20, 30, 0.9));
  border: 3px solid rgba(76, 175, 80, 0.3);
  border-radius: 15px;
  transition: all 0.4s cubic-bezier(0.175, 0.885, 0.32, 1.275);
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  gap: 15px;
  position: relative;
  overflow: hidden;
  box-shadow: 0 8px 20px rgba(0, 0, 0, 0.5);
  /* Make the card responsive while keeping ratio via grid */
  aspect-ratio: 1 / 1;
  min-width: 110px;
  height: auto; /* allow vertical auto-resize */
  max-height: 32vh; /* max card height relative to viewport */
  min-height: 120px; /* minimum for legibility */
  box-sizing: border-box; /* ensure padding/border are included so square sizing matches */
}

.plane-option::before {
  content: '';
  position: absolute;
  top: 0;
  left: -100%;
  width: 100%;
  height: 100%;
  background: linear-gradient(90deg, transparent, rgba(76, 175, 80, 0.2), transparent);
  transition: left 0.5s;
}

.plane-option:hover::before {
  left: 100%;
}

.plane-option:hover {
  background: linear-gradient(145deg, rgba(40, 40, 50, 0.95), rgba(30, 30, 40, 0.95));
  border-color: #4CAF50;
  transform: translateY(-8px) scale(1.05);
  box-shadow: 0 12px 30px rgba(76, 175, 80, 0.4);
}

.plane-option.selected {
  border-color: #00ff00;
  background: linear-gradient(145deg, rgba(76, 175, 80, 0.3), rgba(56, 142, 60, 0.3));
  box-shadow: 0 0 30px rgba(76, 175, 80, 0.6), 0 12px 30px rgba(0, 0, 0, 0.5);
  transform: translateY(-5px);
}

.plane-option.selected::after {
  content: '✓';
  position: absolute;
  top: 10px;
  right: 10px;
  background: #00ff00;
  color: #000;
  width: 30px;
  height: 30px;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-weight: bold;
  font-size: 1.2rem;
  box-shadow: 0 0 15px rgba(0, 255, 0, 0.8);
}

.plane-option img {
  width: 55%;
  height: auto;
  filter: drop-shadow(0 5px 10px rgba(0, 0, 0, 0.5));
  transition: transform 0.3s ease;
  object-fit: contain;
  object-position: center;
  transform: scaleX(1);
}

.plane-option:hover img {
  transform: scale(1.1) rotate(2deg);
}

/* Garantir que todos os aviões apontem para a direita */
.plane-option img {
  transform: scaleX(1);
}

/* Remove per-plane sizing overrides: images will be responsive via % width */
.plane-option[data-plane="1"] img,
.plane-option[data-plane="2"] img,
.plane-option[data-plane="3"] img,
.plane-option[data-plane="4"] img,
.plane-option[data-plane="5"] img,
.plane-option[data-plane="6"] img {
  width: 55% !important;
  height: auto !important;
}

/* Virar apenas o F-39 Gripen (aviao6) */
.plane-option[data-plane="6"] img {
  transform: scaleX(1);
}

.plane-option:hover img {
  transform: scale(1.1) rotate(2deg);
  animation: flipPlane 0.6s ease-in-out;
}

.plane-option[data-plane="6"]:hover img {
  transform: scaleX(1) scale(1.1) rotate(2deg);
  animation: flipPlane 0.6s ease-in-out;
}

@keyframes flipPlane {
  0% {
    transform: rotateY(0deg) scale(1);
  }
  50% {
    transform: rotateY(180deg) scale(1.1);
  }
  100% {
    transform: rotateY(360deg) scale(1.1);
  }
}

.plane-option span {
  color: #fff;
  font-size: 1.1rem;
  font-weight: bold;
  text-transform: uppercase;
  letter-spacing: 1px;
  text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.8);
  white-space: normal;
  text-align: center;
  width: 100%;
  align-self: flex-end;
  margin-top: auto;
}

.start-btn {
  /* Responsive sizing: width, padding, and font scale with viewport size. */
  display: inline-block;
  width: clamp(150px, 40vw, 520px); /* flexible width between 150px and 520px */
  padding: clamp(8px, 1.6vw, 22px) clamp(16px, 5vw, 56px);
  font-size: clamp(0.8rem, 2.2vw, 1.6rem);
  background: linear-gradient(135deg, #4CAF50, #45a049, #66BB6A);
  color: white;
  border: 3px solid #45a049;
  border-radius: clamp(12px, 4vw, 28px);
  cursor: pointer;
  font-weight: 900;
  text-transform: uppercase;
  letter-spacing: 2px;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  transition: all 0.4s ease;
  box-shadow: 0 8px 25px rgba(76, 175, 80, 0.5), inset 0 -3px 0 rgba(0, 0, 0, 0.2);
  position: relative;
  overflow: hidden;
  white-space: nowrap;
  pointer-events: auto; /* ensure clicks/taps are processed */
  touch-action: manipulation; /* favors faster response on touch devices */
}

.start-btn::before {
  content: '';
  position: absolute;
  top: 50%;
  left: 50%;
  width: 0;
  height: 0;
  border-radius: 50%;
  background: rgba(255, 255, 255, 0.3);
  transform: translate(-50%, -50%);
  transition: width 0.6s, height 0.6s;
}

.start-btn:hover::before {
  width: clamp(140px, 80vw, 420px);
  height: clamp(140px, 80vw, 420px);
}

.start-btn:hover {
  background: linear-gradient(135deg, #66BB6A, #4CAF50, #45a049);
  transform: translateY(-3px);
  box-shadow: 0 12px 35px rgba(76, 175, 80, 0.7), inset 0 -3px 0 rgba(0, 0, 0, 0.2);
  border-color: #66BB6A;
}

.start-btn:active {
  transform: translateY(0);
  box-shadow: 0 5px 15px rgba(76, 175, 80, 0.4);
}

.start-btn:disabled {
  background: linear-gradient(135deg, #555, #444);
  cursor: not-allowed;
  opacity: 0.5;
  border-color: #444;
  box-shadow: none;
}

/* Classe utilitária para posicionar o texto do turno do jogador */
.turno-posicionavel {
  position: absolute;
  left: 50%;
  top: 300px; /* Altere este valor para a posição vertical desejada */
  transform: translateX(-50%);
  z-index: 20;
  width: max-content;
  margin: 0;
  padding: 0.2em 1em;
  background: none;
  color: #fff;
  font-size: 1.2em;
  font-weight: 600;
  text-align: center;
  text-shadow: 1px 1px 2px #000a;
  pointer-events: none;
}
