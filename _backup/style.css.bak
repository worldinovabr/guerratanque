/* CSS 3D corner fire + smoke removed per user request.
   Removed selectors: #css-corner-fire, .css-corner-fire and related keyframes/pseudo-elements.
   If you want to restore later, revert this section from version control. */

/* Third corner fire: positioned to the right of the original corner-fire; same visual size */
#css-corner-fire-3 {
  position: fixed;
  /* place just to the right of the original default left (28px) + container width (90px) + small gap */
  left: calc(28px + 90px + 12px);
  bottom: 24px;
  width: 90px; /* same as #css-corner-fire */
  height: 28px; /* same as #css-corner-fire */
  pointer-events: none;
  z-index: 1000;
  transform-style: preserve-3d;
  perspective: 700px;
}

/* Ensure children reuse the same .css-corner-fire rules (we rely on shared classes) */


/* Scene clouds: lightweight 3D DOM layer for subtle atmospheric depth */
.scene-clouds {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 40vh; /* only occupy top portion so it doesn't cover gameplay area */
  pointer-events: none;
  z-index: 60; /* increase so clouds are visibly above smoke/fire and other canvas layers */
  transform-style: preserve-3d;
  perspective: 90px;
  overflow: visible;
}

.scene-clouds .cloud {
  position: absolute;
  bottom: 8vh; /* anchor near top of canvas area */
  transform-origin: center center;
  will-change: transform, opacity;
  overflow: visible;
}

/* inner visual layer slides horizontally; outer .cloud holds depth transforms */
.scene-clouds .cloud .cloud-inner {
  position: absolute;
  inset: 0; /* fill parent */
  /* Create multiple puff lobes via layered radial-gradients (highlights -> midtones -> shadow)
     plus a subtle bluish core to match the reference image. */
  background-image:
    radial-gradient(circle at 22% 38%, rgba(255,255,255,1) 0%, rgba(250,250,250,0.98) 18%, rgba(240,240,240,0.95) 30%, rgba(0,0,0,0) 60%),
    radial-gradient(circle at 50% 62%, rgba(255,255,255,0.98) 0%, rgba(245,245,245,0.96) 16%, rgba(225,225,230,0.92) 34%, rgba(0,0,0,0) 70%),
    radial-gradient(circle at 78% 44%, rgba(255,255,255,0.96) 0%, rgba(245,245,245,0.94) 14%, rgba(220,220,230,0.9) 32%, rgba(0,0,0,0) 68%),
    radial-gradient(circle at 50% 50%, rgba(180,220,255,0.14) 0%, rgba(180,220,255,0.08) 10%, rgba(0,0,0,0) 50%);
  background-repeat: no-repeat;
  background-size: 58% 62%, 72% 66%, 46% 58%, 100% 100%;
  border-radius: 48% / 60%;
  /* Slightly reduced blur but stronger contrast + subtle saturation for soft colored highlights */
  filter: blur(8px) contrast(1.75) brightness(1.03) saturate(1.05);
  /* external shadow for separation + inset shading for depth */
  box-shadow: 0 14px 36px rgba(0,0,0,0.55), inset 0 -10px 24px rgba(0,0,0,0.22), inset 0 10px 16px rgba(255,255,255,0.6);
  opacity: 0.995;
  pointer-events: none;
  transform-origin: center center;
  /* continuous horizontal slide with integrated subtle vertical sway/rotation */
  animation: cloud-slide var(--cloud-duration, 42000ms) linear infinite;
  background-blend-mode: normal, normal, normal, screen;
}

/* Add extra puffy lobes using pseudo-elements to give 3D stacked feel */
.scene-clouds .cloud .cloud-inner::before,
.scene-clouds .cloud .cloud-inner::after {
  content: '';
  position: absolute;
  pointer-events: none;
  border-radius: 50%;
  filter: blur(10px) contrast(1.6) brightness(1.02);
  background: radial-gradient(circle at 40% 40%, rgba(255,255,255,1) 0%, rgba(240,240,240,0.96) 24%, rgba(200,200,210,0.9) 48%, rgba(0,0,0,0) 100%);
  box-shadow: inset 0 -6px 14px rgba(0,0,0,0.18);
}
.scene-clouds .cloud .cloud-inner::before {
  width: 70%;
  height: 68%;
  left: -8%;
  top: -18%;
  transform: translateZ(10px) scale(1.02);
  opacity: 0.98;

}
.scene-clouds .cloud .cloud-inner::after {
  width: 56%;
  height: 54%;
  right: -12%;
  bottom: -6%;
  transform: translateZ(6px) scale(0.98);
  opacity: 0.94;
}

/* Slight color tint for the core to emulate the blueish shadow in the reference */
.scene-clouds .cloud .cloud-inner .core-tint {
  position: absolute;
  inset: 18% 22% 18% 22%;
  border-radius: 50%;
  background: radial-gradient(circle at 50% 50%, rgba(160,210,255,0.18) 0%, rgba(160,210,255,0.08) 30%, rgba(0,0,0,0) 80%);
  pointer-events: none;
}


/* cluster clouds more tightly and slow horizontal movement for a gentler drift */
.scene-clouds .cloud.depth-1 { transform: translate3d(36vw, 0, 0) scale(1.15); opacity: 0.78; }
.scene-clouds .cloud.depth-2 { transform: translate3d(44vw, 0, -60px) scale(1.00); opacity: 0.65; }
.scene-clouds .cloud.depth-3 { transform: translate3d(50vw, 0, -120px) scale(0.82); opacity: 0.52; }

.scene-clouds .c--lg { left: 42%; width: 120px; height: 42px; }
.scene-clouds .c--md { left: 46%; width: 98px; height: 36px; }
.scene-clouds .c--md2 { left: 50%; width: 78px; height: 32px; }
.scene-clouds .c--sm { left: 54%; width: 62px; height: 26px; }
.scene-clouds .c--sm2 { left: 58%; width: 52px; height: 22px; }

/* per-depth movement tuning: increase durations (slower) for gentler drift */
.scene-clouds .cloud.depth-1 .cloud-inner { --cloud-duration: 56000ms; }
.scene-clouds .cloud.depth-2 .cloud-inner { --cloud-duration: 45000ms; }
.scene-clouds .cloud.depth-3 .cloud-inner { --cloud-duration: 34000ms; }

/* stagger start times so clouds don't all move in sync */
.scene-clouds .cloud:nth-of-type(1) .cloud-inner { animation-delay: -2s; }
.scene-clouds .cloud:nth-of-type(2) .cloud-inner { animation-delay: -1s; }
.scene-clouds .cloud:nth-of-type(3) .cloud-inner { animation-delay: -3s; }
.scene-clouds .cloud:nth-of-type(4) .cloud-inner { animation-delay: -0.5s; }
.scene-clouds .cloud:nth-of-type(5) .cloud-inner { animation-delay: -1.5s; }

/* Subtle animations */

@keyframes cloud-slide {
  /* move left -> right while adding a subtle vertical sway/rotation and opacity shift
     We keep translateX values large so clouds start off-screen left and exit off-screen right. */
  0% {
    transform: translateX(-120vw) translateY(0) rotate(0deg);
    opacity: 0.72;
  }
  25% {
    transform: translateX(-40vw) translateY(-6px) rotate(0.18deg);
    opacity: 0.82;
  }
  50% {
    transform: translateX(20vw) translateY(0) rotate(0deg);
    opacity: 0.86;
  }
  75% {
    transform: translateX(80vw) translateY(-4px) rotate(-0.12deg);
    opacity: 0.8;
  }
  100% {
    transform: translateX(120vw) translateY(0) rotate(0deg);
    opacity: 0.72;
  }
}

/* Reduce intensity on small screens for performance */
@media (max-width: 900px) {
