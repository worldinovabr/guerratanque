/* ======================================================================
   New responsive helper: keep game controls fixed at the bottom of viewport
   without modifying existing configuration classes. Apply by adding
   the class `responsive-bottom` to the `.controls-container` element.
   This preserves all existing styles and only provides an extra override
   to pin the controls to the bottom and reduce obstruction of the canvas.
   ====================================================================== */

/* default: pin controls to bottom (desktop + mobile). Keep it unobtrusive */
.controls-container.responsive-bottom {
  position: fixed !important;
  left: 50% !important;
  bottom: 0px !important;            /* colado na borda inferior */
  transform: translateX(-50%) !important;
  width: auto !important;              /* auto width for desktop */
  max-width: 900px !important;         /* prevent too wide on large screens */
  margin: 0 auto !important;
  border-radius: 0 !important;
  background: none !important; /* sem fundo */
  box-shadow: none !important;
  padding: 10px 20px !important;
  z-index: 1100 !important; /* above canvas and other UI layers */
  display: flex !important;
  flex-direction: row !important;
  justify-content: center !important;
  align-items: center !important;
  gap: 24px !important;
  box-sizing: border-box !important;
}

/* Make coordinate controls more compact inside the fixed bottom bar */
.controls-container.responsive-bottom .coord-controls {
  max-width: 640px !important;
  width: auto !important;
  display: flex !important;
  gap: 10px !important;
  align-items: center !important;
  justify-content: flex-start !important;
  flex-wrap: nowrap !important;
}

.controls-container.responsive-bottom .coord-item {
  display: flex !important;
  flex-direction: row !important;
  gap: 6px !important;
  min-width: 48px !important;
}

.controls-container.responsive-bottom .coord-item label {
  font-size: 12px !important;
  color: #ffcc00 !important;
  min-width: 18px !important;
}

.controls-container.responsive-bottom .coord-item span {
  font-size: 14px !important;
  min-width: 38px !important;
  padding: 3px 6px !important;
}

.controls-container.responsive-bottom .coord-item button {
  width: 26px !important;
  height: 26px !important;
  font-size: 14px !important;
}

/* Action buttons grouped next to coordinates inside the bottom bar */
.controls-container.responsive-bottom .action-buttons {
  margin-left: 12px !important;
  display: flex !important;
  gap: 12px !important;
  align-items: center !important;
}

.controls-container.responsive-bottom .action-buttons button {
  width: 28px !important;
  height: 28px !important;
  border-radius: 8px !important;
  background: linear-gradient(135deg, rgba(76, 175, 80, 0.9), rgba(56, 142, 60, 0.9)) !important;
  border: 2px solid rgba(76, 175, 80, 0.5) !important;
