  }
}
/* Responsivo: aplicar ajustes para todas as telas pequenas (inclui paisagem e retrato) */
@media (max-width: 900px) {
  html, body {
    width: 100vw;
    bottom: 12px !important; /* adjust label position to match reduced padding */
    min-height: 100vh;
    min-width: 100vw;
    overflow-x: hidden;
    background: #1a1a1a;
  }
  body {
  /* Slightly reduce card height by making cards a bit wider relative to height */
  .plane-option {
    aspect-ratio: 1.6 / 1 !important; /* slightly wider -> slightly less vertical height */
    padding: 12px 16px !important; /* reduce vertical padding to further decrease height */
    max-height: 22vh !important; /* cap using viewport height so it adjusts with screen size */
    min-height: 140px !important;
  }
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: flex-start;
    padding: 0 0 15vh 0;
    margin: 0;
    min-height: 100vh;
    width: 100vw;
    box-sizing: border-box;
    overflow-x: hidden;
  }
  
  .smoke {
    position: fixed;
    width: 12px;
    height: 12px;
    background: rgba(140,140,140,0.7);
    border-radius: 50%;
    filter: blur(6px);
    pointer-events: none;
    z-index: 1000;
    transform: translate(-50%, -50%);
    animation: smoke-rise 1200ms linear forwards;
  }

  @keyframes smoke-rise {
    0% {
      transform: translate(-50%, -50%) scale(0.7);
      opacity: 0.9;
    }
    70% {
      opacity: 0.5;
    }
    100% {
      transform: translate(-50%, -220%) scale(1.6);
      opacity: 0;
    }
  }
  canvas {
    display: block;
    margin: 2vw auto 0 auto;
    max-width: 95vw;
    height: auto !important;
    box-sizing: border-box;
  }
  .placar-futebol {
    position: static;
    margin: 1vw 15vw 0 0;     /* ← MARGEM DIREITA AUMENTADA PARA 12vw PARA PUXAR MAIS À ESQUERDA */
    width: 70vw;              /* ← REDUZIDO PARA 70vw */
    max-width: 70vw;          /* ← NUNCA EXCEDE 70% DA TELA */
    height: auto !important;  /* ← ALTURA AUTOMÁTICA PROPORCIONAL */
    min-width: 200px;         /* ← REDUZIDO PARA 200px */
