    height: auto;
    bottom: 0px; /* pin to bottom edge */
    /* remove background card in tight landscape */
    background: none !important;
    box-shadow: none !important;
  }

  .coord-controls {
    flex-direction: row;
    gap: 8px;
    max-width: none;
  }

  .coord-item {
    min-width: 45px;
  }

  .coord-item button {
    width: clamp(18px, 4vw, 26px);
    height: clamp(18px, 4vw, 26px);
    font-size: clamp(10px, 2.5vw, 14px);
  }

  .coord-item span {
    font-size: clamp(10px, 2.5vw, 13px);
    min-width: 25px;
    padding: 1px 4px;
  }

  .coord-item label {
    font-size: clamp(8px, 2vw, 11px);
  }

  .action-buttons button {
    width: clamp(26px, 5.5vw, 35px);
    height: clamp(26px, 5.5vw, 35px);
    font-size: clamp(11px, 2.5vw, 15px);
    background: rgba(51, 51, 51, 0.4) !important;
    border: 2px solid rgba(85, 85, 85, 0.5) !important;
  }
  
  .action-buttons button:hover {
    background: rgba(68, 68, 68, 0.5) !important;
  }

  .btn-icone {
    width: 20px;
    height: 20px;
  }

  .vida-bar {
    width: 25vw;
    height: 12px;
  }

  .soldado-icone {
    width: 20px !important;
    height: 20px !important;
  }
}

/* Ensure Plane Selection layout keeps 'Iniciar Jogo' visible on small landscape phones
   - Make plane options grid scrollable if it would overflow vertically
   - Keep the start button pinned to the bottom of the menu (visible)
   - Reduce columns/layout on very narrow landscape heights to prevent extreme tall cards
*/
@media (max-width: 900px) and (orientation: landscape) {
  #plane-selection-menu {
    display: flex !important;
    flex-direction: column !important;
    justify-content: flex-start !important; /* top align so content stacks naturally */
    align-items: stretch !important; /* make children fill width */
    padding: 12px 12px 18px 12px !important; /* keep some bottom space for start button */
    gap: 10px !important;
    max-height: 100vh !important;
    box-sizing: border-box !important;
  }

  /* Make options area scrollable when it grows too tall so the start button remains visible */
  #plane-selection-menu .plane-options {
    width: 100% !important;
    padding: 10px !important;
    margin: 0 auto !important;
    box-sizing: border-box !important;
    grid-template-columns: repeat(3, 1fr) !important; /* default to 3 columns */
    gap: 10px !important;
    /* Reserve enough space for the header and start button while keeping the grid scrollable */
    max-height: calc(100vh - 120px) !important; /* 120px allowance for title + start button + margins */
    overflow-y: auto !important;
    -webkit-overflow-scrolling: touch !important; /* smooth scrolling on iOS */
  }

  /* Keep the Start button visible by placing it outside the scroll area and making it sticky */
  #start-game-btn {
    position: absolute !important; /* stay visible at bottom of the menu */
    bottom: 12px !important;
    left: 50% !important;
    transform: translateX(-50%) !important;
    z-index: 1003 !important;
    align-self: center !important;
    margin-top: 8px !important;
    box-sizing: border-box !important;
  }

  /* Make cards slightly lower height for landscape phones */
  .plane-option {
    min-height: 84px !important;
    max-height: 140px !important;
    aspect-ratio: 1.6 / 1 !important; /* narrower aspect ratio reduces vertical space */
    padding: 8px 10px !important;
  }

  /* For extremely narrow heights (older phones or UI overlays), switch to 2 columns to reduce height. */
  @media (max-height: 420px) {
    #plane-selection-menu .plane-options {
      grid-template-columns: repeat(2, 1fr) !important;
      max-height: calc(100vh - 110px) !important;
    }
  }

}

/* Make sure the plane menu and its child interactive elements allow pointer events
   to avoid any pseudo-element or other overlay capturing the click on the Start button */
#plane-selection-menu, #plane-selection-menu * {
  pointer-events: auto !important;
}

