  }
}

/* Final desktop override to ensure vertical responsiveness and shorter cards */
@media (min-width: 901px) {
  .plane-options { grid-template-columns: repeat(6, 1fr); gap: 20px; }
  .plane-option {
    padding: 10px 12px !important;  /* compact padding */
    max-height: 22vh !important;    /* responsive cap */
    min-height: 120px !important;   /* ensure visual legibility */
    height: auto !important;         /* use auto height and limited by max-height */
    aspect-ratio: 1.6 / 1 !important; /* keep proportion */
  }
  .plane-option img { width: 60% !important; max-width: 92px !important; height: auto !important; }
  .plane-option span { bottom: 12px !important; }
}

/* Force top-center placement for scoreboard and title across all breakpoints */
/* This makes the placar (score) fixed to the top center and the game title directly below it. */
.placar-futebol {
  position: fixed !important;
  top: clamp(6px, 1.2vh, 14px) !important;
  left: 50% !important;
  transform: translateX(-50%) !important;
  display: flex !important;
  align-items: center !important;
  justify-content: space-between !important;
  gap: clamp(8px, 1.2vw, 18px) !important;
  width: min(92vw, 1100px) !important;
  max-width: 1100px !important;
  padding: clamp(6px, 1vh, 10px) clamp(8px, 3vw, 20px) !important;
  border-radius: clamp(8px, 1.3vh, 14px) !important;
  z-index: 1002 !important;
  box-shadow: var(--glass-shadow) !important;
  background: linear-gradient(180deg, var(--glass-bg-start), var(--glass-bg-end)) !important;
  -webkit-backdrop-filter: blur(var(--glass-blur)) !important;
  backdrop-filter: blur(var(--glass-blur)) !important;
  border: 1px solid var(--glass-border) !important;
  max-height: clamp(48px, 8vh, 64px) !important; /* cap vertical size */
  height: auto !important;
  font-size: clamp(1rem, 1.5vw, 1.3rem) !important;
  box-sizing: border-box !important;
}

.placar-futebol .placar-central{ margin: 0 auto !important; text-align:center !important;}

#game-title {
  position: fixed !important;
  /* Place the title directly below the placar. The calc adds a small gap. */
  top: calc(clamp(6px, 1.2vh, 14px) + clamp(40px, 5vh, 72px) + 8px) !important;
  left: 50% !important;
  transform: translateX(-50%) !important;
  z-index: 1001 !important;
  font-size: clamp(1rem, 2.6vh, 2.4rem) !important;
  letter-spacing: 2px !important;
  margin: 0 !important;
  padding: 0 8px !important;
  color: #FFD54F !important;
  text-shadow: 0 1px 6px rgba(0,0,0,0.9) !important;
  text-align: center !important;
}

/* Small screens need a slightly larger gap so the placar doesn't overlap the title */
@media (max-width: 480px) {
  .placar-futebol { top: 6px !important; padding: 6px 10px !important; gap: 6px !important; }
  #game-title { top: calc(6px + 44px + 8px) !important; font-size: 1.1rem !important; }
}

@media (min-width: 901px) {
  .placar-futebol { top: 12px !important; padding: 10px 18px !important; gap: 12px !important; }
  #game-title { top: calc(12px + 56px + 8px) !important; font-size: 2rem !important; }
}

